<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.election">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/vote"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/vote"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoTitleBar">
        <activity android:name=".CandidateResults"></activity>
        <activity android:name=".PartyResults" />
        <activity android:name=".result_prediction_type" />
        <activity android:name=".DuplicateVoting" />
        <activity android:name=".FinalVoting" />
        <activity android:name=".VotingActivity" />
        <activity android:name=".SuperadminActivity" />
        <activity android:name=".AdminActivity" />
        <activity android:name=".Homepage" />
        <activity android:name=".Firstpage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login"
            android:noHistory="true" />
        <activity android:name=".Register" />
    </application>

</manifest>