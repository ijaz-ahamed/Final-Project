        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/PBDparty2" />

    <ProgressBar
        android:id="@+id/PBDparty4"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
